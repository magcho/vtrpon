{
  "name": "VTRPON2",
  "version": "2.3.4",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "node test/statecontrol.test.js",
    "npmRebuild": false
  },
  "keywords": [],
  "author": "Tetsu Suzuki",
  "license": "GPL-3.0-or-later",
  "description": "VTRPON2",
  "dependencies": {
    "atem-connection": "^3.6.0",
    "fix-webm-duration": "^1.0.6",
    "fluent-ffmpeg": "^2.1.3",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.2",
    "winax": "^3.6.1"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^24.3.0",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "appId": "pondashi.com-vtrpon",
    "win": {
      "icon": "assets/icons/icon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "assets/icons/icon_512x512.png"
    },
    "extraResources": [
      {
        "from": "src/assets/ffmpeg.exe",
        "to": "ffmpeg.exe"
      },
      {
        "from": "src/assets/ffmpeg",
        "to": "ffmpeg"
      },
      {
        "from": "src/assets/ffprobe.exe",
        "to": "ffprobe.exe"
      },
      {
        "from": "src/assets/ffprobe",
        "to": "ffprobe"
      }
    ]
  }
}
